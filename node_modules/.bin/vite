#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/fernandonikolic/bitcoin-perception/node_modules/.pnpm/vite@6.3.6_@types+node@22.10.2_jiti@1.21.6_terser@5.39.0_tsx@4.20.3_yaml@2.6.1/node_modules/vite/bin/node_modules:/Users/fernandonikolic/bitcoin-perception/node_modules/.pnpm/vite@6.3.6_@types+node@22.10.2_jiti@1.21.6_terser@5.39.0_tsx@4.20.3_yaml@2.6.1/node_modules/vite/node_modules:/Users/fernandonikolic/bitcoin-perception/node_modules/.pnpm/vite@6.3.6_@types+node@22.10.2_jiti@1.21.6_terser@5.39.0_tsx@4.20.3_yaml@2.6.1/node_modules:/Users/fernandonikolic/bitcoin-perception/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/fernandonikolic/bitcoin-perception/node_modules/.pnpm/vite@6.3.6_@types+node@22.10.2_jiti@1.21.6_terser@5.39.0_tsx@4.20.3_yaml@2.6.1/node_modules/vite/bin/node_modules:/Users/fernandonikolic/bitcoin-perception/node_modules/.pnpm/vite@6.3.6_@types+node@22.10.2_jiti@1.21.6_terser@5.39.0_tsx@4.20.3_yaml@2.6.1/node_modules/vite/node_modules:/Users/fernandonikolic/bitcoin-perception/node_modules/.pnpm/vite@6.3.6_@types+node@22.10.2_jiti@1.21.6_terser@5.39.0_tsx@4.20.3_yaml@2.6.1/node_modules:/Users/fernandonikolic/bitcoin-perception/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vite/bin/vite.js" "$@"
else
  exec node  "$basedir/../vite/bin/vite.js" "$@"
fi
